{
    "name": "url detector",
    "description": "detect shoping url",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage","activeTab","scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/mee16.png",
            "32": "/images/mee32.png",
            "48": "/images/mee48.png",
            "128": "/images/mee128.png"
        }
    },
    "icons": {
        "16": "/images/mee16.png",
        "32": "/images/mee32.png",
        "48": "/images/mee48.png",
        "128": "/images/mee128.png"
    }
}