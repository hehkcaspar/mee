{
  "name": "Chrome Extension for Me",
  "description": "Build with react",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
  },
  "permissions": [
    "activeTab", "storage", "tabs", "scripting"
  ]
}
